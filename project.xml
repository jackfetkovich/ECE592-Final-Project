<mujoco model="auv_in_fluid">
    <option timestep="0.0001" density="1.225" viscosity="1.8e-5" cone="elliptic"
    noslip_iterations="3"/>

    <asset>
        <texture name="grid" type="2d" builtin="checker" rgb1=".1 .2 .3"
        rgb2=".2 .3 .4" width="300" height="300" mark="edge" markrgb=".2 .3 .4"/>
        <material name="grid" texture="grid" texrepeat="2 2" texuniform="true"
        reflectance=".2"/>
    </asset>
    <worldbody>
        <light name="top" pos="0 0 10"/>
        <geom size="10 10 .01" type="plane" material="grid"/>
        <body name="box">
            <freejoint/>
            <geom name="red_box" type="box" size=".2 .2 .2" rgba="1 0 0 1" pos="0 0 5" mass="0.1"/>
        </body>

    </worldbody>

</mujoco>